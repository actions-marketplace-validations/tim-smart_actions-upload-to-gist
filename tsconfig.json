{
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "src",
    "outDir": "dist",
    "resolveJsonModule": false,
    "skipLibCheck": true,
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "lib": ["ESNext", "DOM"],
    "sourceMap": true,
    "strict": true,
    "target": "ES2022",
    "incremental": true,
    "tsPlusConfig": "./tsplus.config.json",
    "paths": {
      "actions-upload-to-gist/*": ["./src/*.js"]
    },
    "tsBuildInfoFile": "./tsconfig.tsbuildinfo"
  },
  "include": ["src/**/*"]
}
